page{
  background-color: #f4f4f4;
}
.no-block{
  width: 100%;
  display: flex;
  text-align: right;
  position: relative;
  flex-direction: column;
}
.no-nav{
  width: 120rpx;
  height: 40rpx;
  font-size: 26rpx;
  align-items: center;
  line-height: 40rpx;
  display: block;
  margin-top: 50rpx;
  margin-left: 22rpx;
  border-left: 10rpx solid #07c160;
}
.box{
  width: 95%;
  border-radius: 24rpx;
  margin: 0 auto;
  margin-top: 40rpx;
}
.notice-box{
  margin: 0 auto; 
}
.notice{
  float: left;
  width: 33.3333%;
  height: 170rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
  border-right: 1px solid #eee;
}
.notice:hover{
  background-color: #eee;
}
.notice:nth-child(-n+3){
  border-top: 1px solid #eee;
}
.notice:first-child{
  border-top-left-radius: 10px;
}
.notice:last-child{
  border-bottom-right-radius: 10px;
}
.notice:nth-child(3){
  border-top-right-radius: 10px;
}
.notice:nth-child(3n+1){
  border-left: 1px solid #eee;
}
.notice:nth-last-child(3){
  border-bottom-left-radius: 10px;
}
.img{
  width: 60rpx;
  height: 60rpx;
}
.notice-tit{
  font-size: 26rpx;
  margin-top:20rpx;
  color:#666;
}
.enterprise-box{
  width: 30.3333%;
  background-color: #fff;
  height: 170rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  box-sizing: border-box;
  border-radius: 24rpx;
  margin-left: 22rpx;
  margin-top: 40rpx;
}
.en-notice{
  float: left;
  height: 170rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  box-sizing: border-box;
}