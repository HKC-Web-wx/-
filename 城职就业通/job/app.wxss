/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 右箭头> */
.rightArrow{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.rightArrow::after{
    content: "";
    width: 15rpx;
    height: 15rpx;
    border-top:3rpx solid #bbb;
    border-right: 3rpx solid #bbb;
    transform: rotate(45deg);
    display:block;
}
.cell{
    font-size:16px;
}
.cell-item{
    border-bottom: 1px solid #eee;
    padding:20rpx 30rpx;
}