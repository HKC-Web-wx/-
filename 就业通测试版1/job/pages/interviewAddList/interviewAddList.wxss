page{
  height: 100%;
  background-color: #f4f4f4;
}
.add-box{
  width: 100%;
  height: 125rpx;
  background-color: #f4f4f4;
  position: fixed;
  bottom: 0;
}
.add-box button{
  width: 90%;
  background-color: #07c160;
  color: #FFF;
  margin: 16rpx auto;
  border-radius: 10rpx;
}
.add-box button:hover{
  opacity: 0.7;
}
.in-box{
  margin-bottom: 130rpx;
}
.interviewAddList{
  background-color: #fff;
  margin: 20rpx;
  border-radius: 20rpx;
  box-shadow: 2rpx 5rpx 5rpx #ccc;
  position: relative;
}
.interviewAddList_time{
  font-size: 32rpx;
  color: #ccc;
}
.interviewAddList_title{
  font-size: 32rpx;
  color: #ccc;
}
.interviewAddList_position{
  font-size: 32rpx;
  color: #ccc;
}
.interviewAddList_company{
  color: black;
  font-size: 36rpx;
  font-weight: 700;
  margin: 20rpx;
}
.interviewAddList_box{
  margin: 15rpx 15rpx;
  display: inline-block;
  line-height: 50rpx;
}