/* pages/edit_le_fw/edit_le_fw.wxss */
page{
  height: 100%;
  background-color: #f4f4f4;
}
.subox{
padding: 0 20rpx 20rpx;
margin-bottom: 120rpx;
border-top: 15rpx solid #ddd;
}
input{
  font-size: 14px;
  border-bottom: 1px solid #ddd;
  padding: 10rpx;
}
.imagebox{
  width: 100%;
  height: 180rpx;
  border-bottom: 1px solid #ddd;
  padding: 10rpx;
}
.addimage{
  width: 160rpx;
  height: 160rpx;
  border: 1rpx solid #ddd;
  color: #ddd;
  line-height: 140rpx;
  text-align: center;
  font-size: 90px;
  font-weight: 100;
}
.btn-box{
width: 100%;
height: 40px;
border-top: 2px solid #eee;
position: fixed;
bottom: 0;
padding-top: 15rpx;
padding-bottom: 15rpx;
background-color: #fff;
}
.btn-box button{
  width: 90%;
  background-color: #07c160;
  color:#FFF;
}
