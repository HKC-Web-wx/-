/* pages/editResume/editResume.wxss */
page{
  height: 100%;
  background-color: #f4f4f4;
}
.subox{
padding: 0 20rpx 20rpx;
margin-bottom: 50rpx;
border-top: 15rpx solid #ddd;
}
.radio-group{
  font-size: 14px;
  border-bottom: 1px solid #ddd;
  padding: 10rpx;
}
.radio{
  margin-right: 15rpx;
}
input{
  font-size: 14px;
  border-bottom: 1px solid #ddd;
  padding: 10rpx;
}
.disable{
  color: #aaa;
}
.msgtext{
  width: 670rpx;
  height: 260rpx;
  border: 1rpx solid #07c160;
  border-radius: 15rpx;
  margin: 25rpx auto;
  margin-bottom: 0;
  padding: 15rpx;
  box-sizing: border-box;
}
.btn-box{
width: 100%;
height: 40px;
border-top: 2px solid #eee;
padding-top: 15rpx;
padding-bottom: 15rpx;
background-color: #fff;
}
.btn-box button{
  width: 90%;
  background-color: #07c160;
  color:#FFF;
}
.subox view view{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 14px;
  border-bottom: 1px solid #ddd;
  padding: 10rpx;
}