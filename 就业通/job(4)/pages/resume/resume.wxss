.rows{
  width: 100%;
  height: 25rpx;
  background-color: #eee;
}
.stu-name{
  font-size: 34rpx;
  color: #444;
  font-weight: 600;
}
.look-resume{
  width: 200rpx;
  text-align: center;
  border: 1px solid #07c160;
  background-color: #07c160;
  color: #fff;
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 15rpx;
  position: absolute;
  right: 30rpx;
  top: 90rpx;
}
.stu-self{
  padding-right: 20rpx;
}
.edit-info{
  font-size: 27rpx;
  border: 1px solid #888;
  width: 80rpx;
  height: 45rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  color: #888;
  text-align: center;
  line-height: 45rpx;
  border-radius: 20rpx;
}
.upload{
  position: relative;
  padding: 70rpx 20rpx;
}
.upload-resume{
  width: 150rpx;
  height: 150rpx;
  border: 1px solid #888;
  text-align: center;
  line-height: 125rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  font-size: 180rpx;
  font-weight: 500;
  color: #ccc;
}
.open-resume{
  
}
.info-title{
  font-size: 30rpx;
  color: #333;
  position: absolute;
  top: 20rpx;
  left: 15rpx;
  border-left: 2px solid #07c160;
}
.resume-box{
  position: relative;
  padding-top: 50rpx;
}
.resume-info{
  padding: 20rpx;
  font-size: 28rpx;
  color: #666;
  position: relative;
  left: 25rpx;
}
.resume-info view{
  margin-top: 15rpx;
  margin-bottom: 20rpx;
}
.downloadView{
  width:100%;
  height: 70rpx;
  border-bottom:1rpx solid #5FB7EB;
  position: relative;
}
.itemsView{
  height: 100%;
  line-height:70rpx;
  font-size:26rpx;
  color: #5FB7EB;
  margin-left:10rpx;
  display: inline-block;
}
.btn{
  width: 50rpx;
  height: 50rpx;
  color: #5FB7EB;
  font-size: 50rpx;
  text-align: center;
  line-height: 45rpx;
  border-radius: 50%;
  border: 1px solid #5FB7EB;
  position: absolute;
  right: 34rpx;
  top: 50%;
  margin-top: -25rpx;
}
button{
  border-bottom:1rpx solid red;
  border-radius:0;
}
.flie{
  width: 100%;
  height: 200rpx;
  border-bottom: 1px solid #eee;
}